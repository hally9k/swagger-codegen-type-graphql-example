{
  "name": "graphql-swagger",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "start": "TS_NODE_FILES=true nodemon src/index.ts",
    "start:prod": "node dist/index.js",
    "build": "tsc",
    "code-gen": "rm -rf ./typescript-node-client && docker run --rm -v ${PWD}:/local swaggerapi/swagger-codegen-cli generate -i https://api.tether.co.nz/api-docs.json -l typescript-node -o /local/typescript-node-client"
  },
  "dependencies": {
    "apollo-server": "^2.7.0",
    "bluebird": "^3.5.5",
    "graphql": "14.1.1",
    "jwt-decode": "^2.2.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "ts-node": "^8.3.0",
    "type-graphql": "^0.17.4",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@types/graphql": "^14.2.3",
    "@types/jwt-decode": "^2.2.1",
    "@types/bluebird": "^3.5.27",
    "@types/node": "8",
    "@types/request": "^2.48.2",
    "nodemon": "^1.19.1"
  }
}
